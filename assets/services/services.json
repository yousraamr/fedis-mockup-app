{
  "services": [
    {
      "serviceName": "Register",
      "serviceLink": "https://fedis.com.eg/",
      "serviceImage": "https://upload.wikimedia.org/wikipedia/commons/thumb/2/2f/Google_2015_logo.svg/512px-Google_2015_logo.svg.png",
      "formData": [
        {
          "stepTitle": "Register",
          "label": "Let's get you signed up!",
          "type": "description"
        },
        {
          "label": "Full Name",
          "type": "text",
          "id": "fullname",
          "name": "fullname",
          "readOnly": false,
          "required": true,
          "validationMessage": "Full Name is required.",
          "lg": 12,
          "md": 12
        },
        {
          "label": "Email",
          "type": "email",
          "id": "email",
          "name": "email",
          "readOnly": false,
          "required": true,
          "validationMessage": "Please enter a valid email.",
          "regex": "^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$",
          "lg": 12,
          "md": 12
        },
        {
          "label": "Password",
          "type": "password",
          "id": "password",
          "name": "password",
          "readOnly": false,
          "required": true,
          "minLength": 6,
          "validationMessage": "Password must be at least 6 characters.",
          "lg": 12,
          "md": 12
        },
        {
          "id": "submitButton",
          "type": "button",
          "name": "Register",
          "label": "Sign Up",
          "lg": 12,
          "color": "success",
          "action": "submit",
          "arrow": true,
          "bottom": true
        }
      ]
    }
  ]
}